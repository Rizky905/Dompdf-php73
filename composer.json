{
    "name": "dompdf-php73/dompdf",
    "type": "library",
    "description": "DOMPDF is a CSS 2.1 compliant HTML to PDF converter",
    "homepage": "https://github.com/Rizky905/Dompdf-php73",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Rizky Erlangga",
            "email": "erlangga905@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dompdf\\": "src/"
        },
        "classmap": [
            "lib/"
        ]
    },
    "require": {
        "php": ">=5.4.0",
        "ext-gd": "*",
        "ext-dom": "*",
        "ext-mbstring": "*",
        "phenx/php-font-lib": "0.5.*",
        "phenx/php-svg-lib": "0.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.7-dev"
        }
    }
}